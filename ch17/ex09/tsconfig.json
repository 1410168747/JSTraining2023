{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "outDir": "./ts",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "*": ["*", "./*"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
